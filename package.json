{
  "name": "webpack-simple-boilerplate",
  "version": "1.5.5",
  "description": "Webpack 5 boilerplate (SCSS, PostCSS, ESLint, Prettier, Styleint, Babel, React, TS)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "webpack --config-node-env development --config config/webpack/webpack.development.js",
    "watch": "npm run dev -- --watch",
    "start": "webpack serve --config-node-env development --config config/webpack/webpack.development.js",
    "build": "webpack --config-node-env production --config config/webpack/webpack.production.js",
    "generate": "plop",
    "format": "npm run format:eslint && npm run format:stylelint && npm run format:prettier",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:stylelint": "npm run lint:stylelint -- --fix",
    "format:prettier": "npm run lint:prettier -- --write",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier && npm run lint:typescript",
    "lint:eslint": "eslint \"**/*.{js,ts,jsx,tsx,[cm][jt]s}\"",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "lint:prettier": "prettier \"**/*.{json,css,scss,js,ts,jsx,tsx,[cm][jt]s}\" --check",
    "lint:typescript": "tsc --noEmit",
    "debug:eslint": "eslint --print-config \"./src/index.tsx\" > eslint-output-config-ts.json",
    "debug:eslint:js": "eslint --print-config \"babel.config.cjs\" > eslint-output-config-js.json",
    "debug:stylelint": "stylelint \"src/styles/styles.css\" --print-config > stylelint-output-config.json",
    "debug:prettier": "prettier \"./src/index.tsx\" --loglevel debug",
    "semantic-release:local-check": "semantic-release --dry-run",
    "cm": "cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/runtime": "7.28.2",
    "@sentry/react": "10.3.0",
    "clsx": "2.1.1",
    "core-js": "3.45.0",
    "jquery": "4.0.0-beta.2",
    "normalize.css": "8.0.1",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/plugin-transform-runtime": "7.28.0",
    "@babel/preset-env": "7.28.0",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.6.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@sentry/webpack-plugin": "4.0.2",
    "@svgr/webpack": "8.1.0",
    "@types/babel__core": "7.20.5",
    "@types/node": "20.19.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/webpack-bundle-analyzer": "4.7.0",
    "autoprefixer": "10.4.21",
    "babel-loader": "10.0.0",
    "commitizen": "4.3.1",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "copy-webpack-plugin": "13.0.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "csv-loader": "3.0.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-webpack-plugin": "5.0.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "globals": "16.3.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "mini-css-extract-plugin": "2.9.3",
    "plop": "4.0.1",
    "postcss": "8.5.6",
    "postcss-loader": "8.1.1",
    "prettier": "3.6.2",
    "react-refresh": "0.17.0",
    "sass": "1.90.0",
    "sass-loader": "16.0.5",
    "semantic-release": "22.0.6",
    "style-loader": "4.0.0",
    "stylelint": "16.23.1",
    "stylelint-config-recess-order": "7.1.0",
    "stylelint-config-standard": "38.0.0",
