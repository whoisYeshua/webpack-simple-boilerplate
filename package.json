{
    "name": "webpack-simple-boilerplate",
    "version": "1.0.0",
    "description": "Webpack 5 boilerplate (SASS, PostCSS, ESLint, Prettier, Styleint, Babel, React, TS)",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --config config/webpack.development.js",
        "build": "webpack --config config/webpack.production.js",
        "watch": "cross-env NODE_ENV=development webpack --config config/webpack.development.js --watch",
        "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.development.js"
    },
    "keywords": [
        "webpack",
        "webpack 5",
        "webpack boilerplate"
    ],
    "author": "whoisYeshua",
    "license": "ISC",
    "engines": {
        "node": ">=12"
    },
    "devDependencies": {
        "@babel/core": "^7.13.15",
        "@babel/eslint-parser": "^7.13.14",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^9.0.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "css-minimizer-webpack-plugin": "^3.0.2",
        "csv-loader": "^3.0.3",
        "eslint": "^7.24.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.2.0",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unicorn": "^35.0.0",
        "eslint-webpack-plugin": "^3.0.1",
        "html-webpack-plugin": "^5.3.1",
        "mini-css-extract-plugin": "^2.2.0",
        "postcss": "^8.2.10",
        "postcss-loader": "^6.1.1",
        "prettier": "^2.2.1",
        "sass": "^1.32.8",
        "sass-loader": "^12.1.0",
        "style-loader": "^3.2.1",
        "stylelint": "^13.12.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-scss": "^3.19.0",
        "webpack": "^5.28.0",
        "webpack-bundle-analyzer": "^4.4.1",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^4.1.0",
        "webpack-merge": "^5.7.3",
        "xml-loader": "^1.2.1"
    },
    "dependencies": {
        "core-js": "^3.10.1",
        "jquery": "^3.6.0",
        "normalize.css": "^8.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
